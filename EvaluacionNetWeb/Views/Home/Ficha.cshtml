

@{
    ViewData["Title"] = "Ficha";
}

@model Profesores

<h2>Ficha</h2>
<fieldset class="form-horizontal">

    <legend class="label">
        Ficha Docente
    </legend>

    <div class="form-group">
        <label class="control-label col-sm-2" for="rut">Rut:</label>
        <div class="text-muted">@Html.DisplayFor(m=>m.rut)</div>
    </div>

    <div class="form-group">
        <label class="control-label col-sm-2" for="nombrecompleto">Nombre Completo:</label>
        <div class="text-muted">@Html.DisplayFor(m => m.nombrecompleto)</div>
    </div>

    <div class="form-group">
        <label class="control-label col-sm-2" for="titulo">Titulo:</label>
        <div class="text-muted">@Html.DisplayFor(m => m.titulo)</div>
    </div>

    <div class="form-group">
        <label class="control-label col-sm-2" for="gradoprofesional">Grado profesional:</label>
        <div class="text-muted">@Html.DisplayFor(m => m.gradoprofesional)</div>
    </div>

    
</fieldset>
